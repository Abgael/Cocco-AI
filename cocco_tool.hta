					<li onclick="setMenuTab('s_action1',6)"><span>action1</span></li>
					<li onclick="setMenuTab('s_action2',7)"><span>action2</span></li>
					<li onclick="setMenuTab('s_action3',8)"><span>action3</span></li>
					<li onclick="setMenuTab('s_common',9)"><span>common</span></li>
